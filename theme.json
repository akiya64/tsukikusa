{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "980px",
			"wideSize": "1200px",
			"blockGap": true
		},
		"spacing": {
			"margin": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "monofur, sans-serif",
					"slug": "monofur",
					"name": "Monofur"
				}
			]
		},
		"color": {
			"palette": [
				{
					"color": "#257bcc",
					"name": "Tsuyukusa",
					"slug": "tsuyukusa"
				},
				{
					"color": "#21a0db",
					"name": "Tsukikusa",
					"slug": "tsukikusa"
				},
				{
					"color": "#274765",
					"name": "Hukaki Hanada",
					"slug": "hukaki-hanada"
				},
				{
					"color": "#3f729f",
					"name": "Nakano Hanada",
					"slug": "nakano-hanada"
				},
				{
					"color": "#618bad",
					"name": "Tsugino Hanada",
					"slug": "tsugino-hanada"
				}
			]
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Site Header",
			"area": "header"
		}
	],
	"styles": {
		"elements": {
			"link": {
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline dotted 0.075em"
					}
				}
			}
		}
	}
}
